{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["ui/.next/**", "backend/dist/**"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
